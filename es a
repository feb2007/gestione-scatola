    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>gestione scatola</title>
</head>
<body>
    <script> 
        let lunghezza = prompt("inserisci lunghezza\n"); //chiedi lunghezza
        let altezza = prompt("inserisci altezza:"); //chiedi altezza
        let profondita = prompt("inserisci profondita: "); // chiedi profondità

        let volume = altezza * lunghezza * profondita; // calcolo volume
        alert("il volume del cubo è" + volume); // visualizza volume del cubo
        console.log("il volume del cubo è: " + volume); // sulla console viene visualizzato il volume
    </script>
</body>
